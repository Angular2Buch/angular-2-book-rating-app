{
  "version": "1.5.3",
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "declaration": false,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "listFiles": true,
    "experimentalDecorators": true,
    "newLine": "LF"
  },
  "exclude": [],
  "atom": {
    "rewriteTsconfig": true
  },
  "filesGlob": [
    "./**/*.ts",
    "../node_modules/angular2/angular2.d.ts",
    "../node_modules/angular2/bundles/typings/**"
  ],
  "files": [
    "./app/app.ts",
    "./app/components/book-component/book-component.ts",
    "./app/components/book-rating/book-rating.ts",
    "./app/core/books-api.ts",
    "./app/models/book.ts",
    "./test/components/book-component_test.ts",
    "./test/components/book-rating_test.ts",
    "./test/hello-world_test.ts",
    "../node_modules/angular2/angular2.d.ts",
    "../node_modules/angular2/bundles/typings/es6-shim/es6-shim.d.ts",
    "../node_modules/angular2/bundles/typings/jasmine/jasmine.d.ts"
  ]
}
